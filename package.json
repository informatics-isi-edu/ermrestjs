{
  "name": "@isrd-isi-edu/ermrestjs",
  "description": "ERMrest client library in JavaScript",
  "version": "2.1.1",
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 20.0.0",
    "npm": ">=6.0.0"
  },
  "main": "dist/ermrest.js",
  "module": "dist/ermrest.min.js",
  "types": "dist/ermrest.d.ts",
  "files": [
    "dist",
    "js",
    "src",
    "tsconfig.json",
    "vite.config.mts"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "watch": "tsc --watch && vite build --watch",
    "pretest": "make deps-test && make dist-w-deps",
    "deploy": "make deploy",
    "test": "make test",
    "lint": "eslint src js --quiet",
    "lint-w-warn": "eslint src js",
    "format": "prettier --write src",
    "prepare": "node .husky/install.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/informatics-isi-edu/ermrestjs.git"
  },
  "bugs": {
    "url": "https://github.com/informatics-isi-edu/ermrestjs/issues"
  },
  "homepage": "https://github.com/informatics-isi-edu/ermrestjs#readme",
  "keywords": [
    "database",
    "rest",
    "postgresql",
    "library"
  ],
  "dependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/q": "^1.5.8",
    "axios": "1.13.2",
    "handlebars": "4.7.8",
    "lz-string": "^1.5.0",
    "markdown-it": "12.3.2",
    "moment": "2.29.4",
    "moment-timezone": "0.5.48",
    "mustache": "x",
    "q": "1.5.1",
    "spark-md5": "^3.0.0",
    "terser": "^5.44.1",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-compression2": "^2.2.1",
    "vite-plugin-dts": "^4.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.2",
    "@isrd-isi-edu/ermrest-data-utils": "0.0.5",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jasmine": "2.5.3",
    "jasmine-expect": "3.7.1",
    "jasmine-spec-reporter": "2.5.0",
    "nock": "13.5.4",
    "prettier": "^3.7.4",
    "require-reload": "^0.2.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript-eslint": "^8.51.0"
  }
